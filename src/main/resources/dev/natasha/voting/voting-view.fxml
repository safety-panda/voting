<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="291.0" prefWidth="320.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.natasha.voting.controllers.VotingAppController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="labelText" alignment="CENTER" prefHeight="19.0" prefWidth="222.0" text="Who would you like to vote for?">
      <font>
         <Font size="15.0" />
      </font>
   </Label>
   <ButtonBar buttonOrder="" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="159.0">
     <buttons>
          <Button alignment="CENTER" onAction="#onCandidateOneButtonClick" prefHeight="25.0" prefWidth="117.0" text="Candidate One" translateX="-15.0">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
         </Button>
       <Button alignment="CENTER" mnemonicParsing="false" onAction="#onCandidateTwoButtonClick" text="Candidate Two" translateX="-10.0">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
         </Button>
     </buttons>
   </ButtonBar>

    <Label fx:id="candidateSelectionText" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="378.0" />
</VBox>
